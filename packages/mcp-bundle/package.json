{
  "name": "@omni-eye/mcp-bundle",
  "version": "0.1.0",
  "description": "MCP Native Messaging adapter, core protocol utilities, and server SDK.",
  "private": false,
  "type": "commonjs",
  "config": {
    "extensionId": "mdeeiialcgmjlcokkpeeghinjpdcehja"
  },
  "main": "dist/cli/index.js",
  "bin": {
    "mcp-adapter": "./bin/mcp-adapter.js"
  },
  "scripts": {
    "build": "rimraf dist && rollup --config rollup.config.js",
    "register-native-host": "TEST=true node ./scripts/register-native-host.js",
    "unregister-native-host": "TEST=true node ./scripts/unregister-native-host.js",
    "postinstall": "node ./scripts/register-native-host.js",
    "preuninstall": "node ./scripts/unregister-native-host.js"
  },
  "files": [
    "dist",
    "bin",
    "scripts/*.js",
    "README.md"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^20.6.0",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.1",
    "rollup": "^2.61.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "yargs": "^18.0.0"
  }
}
