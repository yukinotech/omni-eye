{
  "name": "@omni-eye/mcp-bundle",
  "version": "0.1.0",
  "description": "MCP Native Messaging adapter, core protocol utilities, and server SDK.",
  "private": false,
  "type": "commonjs",
  "config": {
    "extensionId": "mdeeiialcgmjlcokkpeeghinjpdcehja"
  },
  "bin": {
    "mcp-adapter": "./bin/mcp-adapter",
    "mcp-bundle": "./dist/mcp-server/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.json && node ./src/scripts/add-shabang.js",
    "mcp": "npm run build && node ./dist/mcp-server/index.js",
    "register-native-host": "TEST=true node ./scripts/register-native-host.js",
    "unregister-native-host": "TEST=true node ./scripts/unregister-native-host.js",
    "postinstall": "node ./scripts/register-native-host.js",
    "preuninstall": "node ./scripts/unregister-native-host.js"
  },
  "files": [
    "dist",
    "bin",
    "scripts/*.js",
    "README.md"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^20.6.0",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.1",
    "rollup": "^2.61.1",
    "typescript": "5.4.2",
    "zod": "3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.18.2",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "axios": "^1.12.2",
    "fastify": "^5.6.1",
    "tslib": "^2.8.1",
    "unfluff": "^3.2.0",
    "yargs": "^18.0.0"
  }
}
